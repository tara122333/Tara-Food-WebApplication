{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "nginx",
        "image": "tarachand122333/zomato-shapeai-nginx",
        "hostname": "nginx",
        "essentials": true,
        "postMappings": [{ "hostPort": 80, "containerPort": 80 }],
        "links": ["client", "nodeserver"],
        "memory": 128
      },
      {
        "name": "client",
        "image": "tarachand122333/zomato-shapeai-client",
        "hostname": "client",
        "essentials": false,
        "memory": 300
      },
      {
        "name": "nodeserver",
        "image": "tarachand122333/zomato-shapeai-server",
        "hostname": "nodeserver",
        "essentials": false,
        "memory": 300
      }
    ]
  }
  
  